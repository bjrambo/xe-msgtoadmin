<query id="insertLog" action="insert">
	<tables>
		<table name="msgtomanager_log" />
	</tables>
	<columns>
		<column name="log_srl" var="log_srl" notnull="notnull" />
		<column name="sender_srl" var="sender_srl" default="0" />
		<column name="receiver_srl" var="receiver_srl" notnull="notnull" />
		<column name="message_srl" var="message_srl" notnull="notnull" />
		<column name="related_srl" var="related_srl" notnull="notnull" />
		<column name="title" var="title" notnull="notnull" />
		<column name="content" var="content" notnull="notnull" />
		<column name="nick_name" var="nick_name" notnull="notnull" />
		<column name="email" var="email" notnull="notnull" />
	</columns>
</query>
